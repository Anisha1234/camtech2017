                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="../logout">
                                LOG OUT
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>

<%
var get_weeks = function(lmp){
	var currDate = new Date();
	var lastDate = new Date(lmp);
	return Math.round((currDate - lastDate)/1000/60/60/24/7);
}
%>
        <div class="content">
            <div class="container-fluid">
              <div class="row">
								<% for(var i=0;i<org.patients.length;i++){
									var patient = org.patients[i];
								%>
								<a href="/organisation/patient/<%= patient._id%>" style="color:inherit;">
                  <div class="col-md-6">
                      <div class="card">
                          <div class="header">
                              <h2 class="title"><%= patient.name %></h2>
                          </div>
                          <div class="content">
														<h4>Contact : <%= patient.contact %></h4>
														<h4>Current Week : <%= get_weeks(patient.lmp) %></h4>
                          </div>
                      </div>
                  </div>
								</a>
									<%
								}
									%>
              </div>
            </div>
        </div>

<%- include footer.ejs %>

    </div>
</div>




    <!--   Core JS Files   -->
    <script src="../assets/js/jquery-1.10.2.js" type="text/javascript"></script>
	<script src="../assets/js/bootstrap.min.js" type="text/javascript"></script>

